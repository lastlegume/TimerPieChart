{
  "manifest_version": 3,
  "name": "Timer Pie Chart",
  "version": "1.0.2",
  "description": "Adds a pie chart to the default timer on Google",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.google.com/*"],
      "js": ["modifyTimer.js"]
    }
  ]
}